package com.lee.strategy;

public class MinuStrategy implements ICalculate {

	public MinuStrategy(String cal)
	{
		
	}
	
	public boolean isSupport(String calc) {
		if ("-".equals(calc))
		{
			return true;
		}
		return false;
	}

	public int executeCal(int rNum, int lNum) {
		
		return rNum - lNum;
	}

}
