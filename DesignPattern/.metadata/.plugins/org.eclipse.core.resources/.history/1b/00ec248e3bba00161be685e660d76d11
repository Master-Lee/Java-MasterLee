package com.lee.strategy;

public class PlusStrategy implements ICalculate {

	public String calc;
	
	public PlusStrategy(String calc)
	{
		this.calc = calc;
	}
	
	public boolean isSupport(String calc) {
		if ("+".endsWith(calc))
		{
			return true;
		}
		return false;
	}

	public int executeCal(int rNum, int lNum) {
		
		if (!isSupport(calc))
		{
			return -1;
		}
		
		return rNum + lNum;
	}

}
